{
  "version": 2,
  "builds": [
    { "src": "backend/index.ts", "use": "@vercel/node" },
    { "src": "backend/api/billing/webhook.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/billing/webhook", "dest": "backend/api/billing/webhook.ts" },
    { "src": "/api/(.*)", "dest": "backend/index.ts" }
  ]
} 